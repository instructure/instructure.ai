{
  "name": "@instructure.ai/aiinfo",
  "version": "3.0.0-alpha.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure.ai",
    "directory": "packages/aiinfo"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.mjs",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs",
      "require": "./src/index.cjs",
      "default": "./src/index.mjs"
    }
  },
  "module": "./src/index.mjs",
  "types": "./src/index.d.ts",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "pnpm cache:update && pnpm typecheck && pnpm build:lib && pnpm lint && pnpm package",
    "build:lib": "tsdown",
    "cache:clear": "CLEAR=1 vite-node ./scripts/clearCache.mts",
    "cache:update": "vite-node ./scripts/updateCache.mts",
    "lint": "oxfmt && oxlint --quiet --type-aware --fix --fix-suggestions",
    "package": "pnpm pack --pack-destination ./src",
    "test": "pnpm vitest run",
    "typecheck": "tsgo --noEmit",
    "write:all": "WRITE=1 vite-node ./scripts/writeEntries.mts",
    "write:entries": "WRITE=1 vite-node ./scripts/writeEntries.mts --entries",
    "write:barrel": "WRITE=1 vite-node ./scripts/writeEntries.mts --barrel"
  },
  "dependencies": {},
  "devDependencies": {
    "@instructure.ai/shared-configs": "workspace:^",
    "@instructure/ui-instructure": "^11.2.0",
    "@types/papaparse": "^5.3.16",
    "ansis": "^4.2.0",
    "papaparse": "^5.5.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
