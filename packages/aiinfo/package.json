{
  "dependencies": {
    "@instructure/shared-types": ">=10.22.0 < 12",
    "@instructure/ui-buttons": ">=10.22.0 < 12",
    "@instructure/ui-instructure": ">=10.22.0 < 12"
  },
  "devDependencies": {
    "@instructure.ai/shared-configs": "workspace:^",
    "@types/papaparse": "^5.3.16",
    "ansis": "^4.2.0",
    "papaparse": "^5.5.3",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "*.md"
  ],
  "module": "./dist/index.mjs",
  "name": "@instructure.ai/aiinfo",
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/instructure/instructure.ai",
    "directory": "packages/aiinfo"
  },
  "scripts": {
    "build": "pnpm cache:update && pnpm build:vite && pnpm lint --write && pnpm typecheck && pnpm package",
    "build:vite": "vite build",
    "cache:clear": "CLEAR=1 vite-node ./scripts/clearCache.mts",
    "cache:update": "UPDATE=1 vite-node ./scripts/updateCache.mts",
    "lint": "pnpm biome check",
    "package": "pnpm pack --pack-destination ./dist",
    "test": "pnpm vitest run --config=./vitest.config.mts",
    "typecheck": "tsgo --noEmit"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "1.2.19"
}
